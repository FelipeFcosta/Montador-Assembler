
INIT: INPUT N1

INPUT N2
INPUT N3
LOAD N1
SUB N2
JMPN PROX

COPY N1,T
COPY N2,N1
COPY T,N2

PROX: LOAD N1
SUB N3
JMPN ULT

COPY N1,T
COPY N3,N1
COPY T,N3

ULT: LOAD N2
SUB N3
JMPN OUT
COPY N2,T
COPY N3,N2
COPY T,N3

OUT: OUTPUT N1
OUTPUT N2
OUTPUT N3
JMPP FIM

FIM: INPUT N4
LOAD n4
JMPP INIT
JMPZ INIT
STOP


N1: SPACE
N2: SPACE
N3: SPACE
N4:SPACE
T: SPACE
